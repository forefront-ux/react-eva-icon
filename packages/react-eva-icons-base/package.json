{
  "name": "@forefront-ux/react-eva-icons-base",
  "version": "2.0.5",
  "description": "Transpile Eva Icons' SVG Content to React Component",
  "scripts": {
    "build": "yarn build:svg && yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:copy-files && yarn build:css",
    "prebuild": "rimraf ./build",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src/generated --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src/generated --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src/generated --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:svg": "node -r esm scripts/build.js",
    "build:copy-files": "node scripts/copy-files.js",
    "precss": "node scripts/build-eva-css.js prepare",
    "build:css": "cd src/icons/eva-icons && npm i && npm run icons:prepare && npx webpack --config webpack.evacss.config.js",
    "postcss": "node scripts/build-eva-css.js post && cd src/icons/eva-icons && git checkout .",
    "release": "yarn build && npm publish build --tag latest",
    "typescript": "tslint -p tsconfig.json \"src/generated/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forefront-ux/react-eva-icons.git"
  },
  "keywords": [
    "eva",
    "icons",
    "svg",
    "react"
  ],
  "author": "Kuma Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forefront-ux/react-eva-icons/issues"
  },
  "homepage": "https://github.com/forefront-ux/react-eva-icons#readme",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/runtime": "^7.4.5",
    "@svgr/core": "^4.3.0",
    "@svgr/plugin-jsx": "^4.3.0",
    "@svgr/plugin-prettier": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "camelcase": "^5.3.1"
  }
}
